        "ts-loader": "^6.2.1",
        "typescript": "3.7.2",
        "webpack": "^4.39.1"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "main": "dist/index.js",
    "files": [
        "dist/*"
    ],
    "nyc": {
        "excludeAfterRemap": false,
        "exclude": [
            "**/*.spec.js"
        ],
        "include": [
            "**/components/**"
        ]
    }
}
