{
    "name": "uw-metadata-react-component-library",
    "version": "0.1.0",
    "license": "MIT",
    "description": "React Component Library for gathering Scripture Burrito Metadata from Gitea repositories",
    "homepage": "https://hellow-dcs-react-component-library.netlify.com/",
    "repository": {
        "type": "git",
        "url": "https://github.com/unfoldingWord-box3/hello-dcs-react-component-library"
    },
    "private": false,
    "scripts": {
        "start": "styleguidist server",
        "build": "styleguidist build",
        "prepublishOnly": "rm -fr ./dist & babel ./src --out-dir ./dist -s inline --extensions \".ts,.js\"",
        "cypress:run": "cypress run",
        "test:e2e": "NODE_ENV=test start-test 6060 cypress:run && nyc report --reporter=json-summary",
        "test:unit": "NODE_ENV=test jest ./src/core --coverage && cat ./coverage/lcov.info | coveralls",
        "test": "nyc --exclude-after-remap=false npm run test:e2e",
        "create-coverage-badge": "bash scripts/create-badge-json.sh"
    },
    "dependencies": {
        "ajv": "^6.12.0",
        "base-64": "0.1.0",
        "fs-extra": "^9.0.0",
        "gitea-react-toolkit": "^1.0.0-rc.2",
        "js-yaml-parser": "^1.0.0",
        "json-schema-ref-parser": "^9.0.1",
        "net": "^1.0.2",
        "prop-types": "15.7.2",
        "react-json-view": "^1.19.1",
        "rjsf-material-ui": "^0.3.9",
        "semver": "^7.3.2",
        "tls": "^0.0.1",
        "ts-node": "^8.8.2"
    },
    "peerDependencies": {
        "@material-ui/styles": "^4.9.10",
        "@material-ui/core": "^4.7.0",
        "@material-ui/icons": "^4.5.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "devDependencies": {
        "@babel/cli": "7.8.4",
        "@babel/core": "7.7.2",
        "@babel/preset-env": "7.7.1",
        "@babel/preset-react": "7.0.0",
        "@babel/preset-typescript": "7.7.2",
        "@cypress/code-coverage": "1.14.0",
        "@date-io/core": "^1.3.6",
        "@istanbuljs/nyc-config-typescript": "0.1.3",
        "@material-ui/core": "^4.7.0",
        "@material-ui/icons": "^4.5.1",
        "@types/base-64": "0.1.3",
        "@types/utf8": "2.1.6",
        "@typescript-eslint/eslint-plugin": "2.7.0",
        "@typescript-eslint/parser": "2.7.0",
        "babel-eslint": "10.0.3",
        "babel-jest": "24.9.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-istanbul": "5.2.0",
        "babel-plugin-transform-class-properties": "6.24.1",
        "coveralls": "3.0.7",
        "cypress": "3.6.1",
        "eslint": "6.6.0",
        "eslint-config-prettier": "6.5.0",
        "eslint-plugin-chai-friendly": "0.5.0",
        "eslint-plugin-cypress": "2.7.0",
        "eslint-plugin-mdx": "1.0.1",
        "eslint-plugin-react": "7.19.0",
        "immutability-helper": "^3.0.0",
        "immutable": "^3.8.1",
        "install-peers-cli": "^2.2.0",
        "istanbul-lib-coverage": "2.0.5",
        "jest": "24.9.0",
        "lorem-ipsum": "2.0.1",
        "markdown-translatable": "^0.11.0",
        "moment": "2.24.0",
        "nyc": "14.1.1",
        "path": "^0.12.7",
        "react": "^16.13.1",
        "react-docgen-typescript": "^1.16.2",
